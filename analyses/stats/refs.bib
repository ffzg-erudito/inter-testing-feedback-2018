}

@article{byrd_adult_1993,
	title = {{ADULT} {AGE}-{DIFFERENCES} {IN} {THE} {ABILITY} {TO} {WRITE} {PROSE} {PASSAGES}},
	volume = {19},
	issn = {0360-1277},
	doi = {10.1080/0360127930190502},
	abstract = {Although there has been a great deal of work investigating age-related decrements in the ability to read and remember prose material, there has been almost no research investigating age differences in the ability to write prose passages. Accordingly, this study asked young and older adults to write short essays on three topics thought to vary in the degree of difficulty needed to complete each task: (1) a descriptive report, (2) a report containing generalized comparisons, and (3) a report containing persuasive arguments. It was thought that the use of these three types of writing assignments would allow for an examination of the manner in which age affects the various linguistic skills necessary to complete writing tasks of varying degrees of difficulty. Analyses of the lexical features of the texts (e.g., passage length, word frequency, etc.) revealed that there were few differences between the texts produced by either age group in any of the writing conditions. Syntactic (e.g., number of correctly produced verbs and sentences) and structural (eg., degree of cohesion in the text) differences in the texts were also examined. In general, it was found that there were few age differences in the syntax used by either age group, but older adults produced texts that were less cohesive than those of younger adults. There were, however, considerable semantic differences (i.e., differences in the topics of the sentences used in the texts) found in the passages produced by the young and older adults. In general, the sentences produced by older adults tended to include more references to concrete objects, whereas the sentences produced by younger adults made greater use of generalized themes and abstract topics. The texts produced by the two age groups were also analyzed in terms of individual differences in the cognitive skills and life circumstances of the participants. It was found that individual differences in both educational level and working memory seemed to be primarily responsible for the differences in the writing styles of young and older adults.},
	number = {5},
	journal = {Educational Gerontology},
	author = {BYRD, M.},
	month = aug,
	year = {1993},
	note = {WOS:A1993LT54600002},
	keywords = {comprehension, elderly adults, language, life-span changes, memory, old-age, recall, syntactic complexity},
	pages = {375--396}
}

@article{wright_knowledge_1993,
	title = {{KNOWLEDGE} {OF} {TEXT} {COHERENCE} {AND} {EXPOSITORY} {WRITING} - {A} {DEVELOPMENTAL}-{STUDY}},
	volume = {85},
	issn = {0022-0663},
	doi = {10.1037//0022-0663.85.1.152},
	abstract = {This study attempted to relate coherence in the expository writing of 4th graders, 8th graders, and college students to their knowledge of the requirements of global coherence in expository texts. Subjects performed a sentence-identification task designed to assess such knowledge and wrote 2 expository essays. Significant positive correlations were found between sentence identification and 3 measures of the rated coherence of the subjects' essays. With one exception, these correlations remained significant when grade was partialed out. Moreover, performance on the sentence-identification and essay tasks improved from 4th to 8th grade. Scores on a holistic measure of essay coherence also improved through college age. Thus, it is likely that over a wide age range, knowledge of the requirements of global coherence in expository essays is a factor in one's ability to produce a coherent essay.},
	number = {1},
	journal = {Journal of Educational Psychology},
	author = {WRIGHT, RE and ROSENBERG, S.},
	month = mar,
	year = {1993},
	note = {WOS:A1993KQ00100014},
	keywords = {awareness, play, students, young writers},
	pages = {152--158}
}

@article{graesser_theory_1993,
