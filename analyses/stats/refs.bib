@article{reed_examination_2016,
	title = {An {Examination} of {Text} {Complexity} as {Characterized} by {Readability} and {Cohesion}},
	volume = {84},
	issn = {0022-0973},
	doi = {10.1080/00220973.2014.963214},
	abstract = {To better understand dimensions of text complexity and their effect on the comprehension of adolescents, 103 high school seniors were randomly assigned to 4 groups. Each group read versions of the same 2 informational passages and answered comprehension test items targeting factual recall and inferences of causal content. Group A passages had a challenging readability level and high cohesion; Group B passages had an easier readability and low cohesion; Group C passages had a challenging readability level and low cohesion; and Group D passages had an easier readability and high cohesion. Students in Group D significantly outperformed students in Group C (g = 0.78). Although the effect sizes of comparisons among all groups ranged from g = 0.13 to 0.73, no other comparisons were statistically significant. Results indicate that adolescents' reading comprehension is dually influenced by a text's readability and cohesion. Implications for matching readers to instructional text are discussed.},
	number = {1},
	journal = {Journal of Experimental Education},
	author = {Reed, Deborah K. and Kershaw-Herrera, Sarah},
	month = jan,
	year = {2016},
	note = {WOS:000364334700004},
	keywords = {coh-metrix, memory, knowledge, reading-comprehension, information, coherence, ability, reading comprehension, adolescents, readability, core state standards, landscape model, causal cohesion, students comprehension, text   complexity},
	pages = {75--97}
}

@incollection{rakhimova_chess_2016,
	title = {{CHESS} {TERMINOLOGY} {AS} {A} {TOOL} {OF} {TEXT} {ORGANIZATION} ({ON} {THE} {EXAMPLE} {OF} {VVNABOKOV}'{S} {NOVEL} "{THE} {DEFENSE}")},
	isbn = {978-619-7105-72-8},
	abstract = {The article analyses the use of chess terminology in the novel "The Defense" by V.V.Nabokov. The conducted research allowed to reveal not only the cases of usual but also of occasional use of chess terms. Textocentric analysis led to the conclusion that the "korol" and "kon'" lexemes are used not only in their direct meaning but also figuratively, that allows the author to give a mediated characteristic to the personage. The inclusion of chess terms also allows to recreate the atmosphere of the game, which is very important in the "game" text. In addition, the chess terms, entering into the syntagmatic and paradigmatic relations run through the fabric of the novel and form synonymous, antonymous and thematic repetitions which help to achieve the coherence of the text. It is noteworthy that Nabokov uses synonymous variants of the names of the figures, borrowed from different intermediary languages. Along with Western versions Arabic terms are also used: koroleva - ferz; tour - poushka - ladya.},
	booktitle = {Sgem 2016, {Bk} 1: {Psychology} and {Psychiatry}, {Sociology} and {Healthcare}, {Education} {Conference} {Proceedings}, {Vol} {Iii}},
	author = {Rakhimova, Dinara and Yusupova, Zulfiya and Fattakhova, Nailya},
	year = {2016},
	note = {WOS:000395722500013},
	keywords = {text, chess terminology, game, Nabokov},
	pages = {89--95}
}

@article{wong_tracking_2016,
	title = {Tracking reading strategy utilisation through pupillometry},
	volume = {32},
	issn = {1449-5554},
	doi = {10.14742/ajet.3096},
	abstract = {Explicit reading strategies help low-knowledge readers make the inferences necessary to comprehend expository texts. Self-explanation is a particularly effective strategy, but it is challenging to monitor how well a reader is applying self-explanation without requiring the reader to externalise the self-explanations being generated. Studies have shown that different reading strategies vary in the amount of cognitive control required as well as the engagement of brain regions involved in internally-directed attention. Pupil diameter is related to task engagement and cognitive control via the brain's locus coeruleus-norepinephrine system. Therefore, pupil diameter could be a method to unobtrusively measure a reader's use of self-explanation. The current study assessed whether pupil diameter can be used to distinguish between the use of different reading strategies and whether it is linked to the quality and effectiveness of the strategy in terms of learning gains. Participants reread, paraphrased, and self-explained texts while pupil diameter was recorded, and completed comprehension tests. Average pupil diameter differed between all three reading strategies, and pupil diameter was related to learning gains and the quality of strategy use. The results suggest that pupil diameter could be used to track effective reading strategy utilisation.},
	number = {6},
	journal = {Australasian Journal of Educational Technology},
	author = {Wong, Aaron Y. and Moss, Jarrod and Schunn, Christian D.},
	year = {2016},
	note = {WOS:000392639900005},
	keywords = {comprehension, students, knowledge, text coherence, self-explanations, adaptive gain, cognitive control, eye-movements, istart},
	pages = {45--57}
}

@incollection{zu_comparative_2016,
	title = {A {Comparative} {Study} on {Textual} {Cohesion} in {Chinese} and {English} {Text} of {Moment} in {Peking}},
	volume = {51},
	isbn = {978-981-09-7911-9},
	abstract = {Textual cohesion is a rather complex matter including so many different cohesive devices which involve phone, words, phrase, sentence, text and some rhetorics. In 1976, the book Cohesion in English written by Halliday and Hasan was published which indicated the theory of cohesion was established. In this book, cohesive devices have been classified into 5 categories: reference, substitution, ellipsis, conjunction and lexical cohesion. However,because this matter involves so many aspects, it is too complicated and it is difficult to explain all the aspects in such limited thousands of words, therefore, the author just choose one cohesive device reference which is used the most frequently as an example to study in detail. As we all know, in Chinese and English, there are different rules in forming a text. Cohesion is the most important one among these rules. In this article, the author will try to compare the reference cohesive devices between Chinese and English Text of Moment in Peking to find out the differences in the using of reference cohesive devices.},
